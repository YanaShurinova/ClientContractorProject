<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="ClientContractorProject">
        <addColumn tableName="CONTRACTOR">
            <column name="ADDRESS" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="ADDRESS" defaultNullValue="''" tableName="CONTRACTOR"/>
    </changeSet>
    <changeSet id="2" author="ClientContractorProject">
        <addColumn tableName="CONTRACTOR">
            <column name="NAME" type="VARCHAR(255)"/>
        </addColumn>

        <addNotNullConstraint columnName="NAME" defaultNullValue="''" tableName="CONTRACTOR"/>
    </changeSet>
</databaseChangeLog>